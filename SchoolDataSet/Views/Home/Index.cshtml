@{
    ViewBag.Title = "Home Page";
    List<SchoolDataSet.Models.Student> students = new List<SchoolDataSet.Models.Student>();
    students = ViewData["students"] as List<SchoolDataSet.Models.Student>;
}

<div class="jumbotron">
    <table>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Last Name</td>
            <td>Sex</td>
            <td>Address</td>
            <td>Phone</td>
        </tr>

        @foreach (var item in students)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.LastName</td>
                <td>@item.Sex</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
            </tr>
        }
    </table>
</div>

